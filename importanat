reate .htaccess file and paste above as it is

<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.html$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-l
RewriteRule . /index.html [L]
</IfModule>

CI/CD

deploy_dev:
  stage: deploy
  tags: 
    - new-client-r2
  environment:
    name: dev
    url: https://rwl-app.artoon.in/
  before_script:
    - eval export PATH=/root/.nvm/versions/node/v14.17.3/bin:${PATH}
  script:
    - npm install --cache
    - CI= npm run-script build 
    - cp /var/www/html/rwl-app/htaccess build/.htaccess
    - cp -r build/* /var/www/html/rwl-app/build_live/
  when: manual
  only:
    - Development


stages:
  - build-image
  - deploy


docker_build_development:
  stage: build-image
  tags:
    - kube-ssh
  script:
    - cp /home/config/dev/ballpool-games-mgp/.env .
    - cp /home/config/dev/ballpool-games-mgp/Dockerfile .
    - cp /home/config/dev/ballpool-games-mgp/node-rolling-update.yml .
    - cp -r /home/config/dev/ballpool-games-mgp/certificate .
    - docker build -f Dockerfile -t $CI_REGISTRY_IMAGE:$CI_COMMIT_SHORT_SHA .
    - echo $CI_REGISTRY_IMAGE
    - docker login -u $CI_REGISTRY_USER -p $CI_REGISTRY_PASSWORD $CI_REGISTRY
    - docker push $CI_REGISTRY_IMAGE:$CI_COMMIT_SHORT_SHA
    - DOCKER_IMG="$CI_REGISTRY_IMAGE:$CI_COMMIT_SHORT_SHA"
    - echo $DOCKER_IMG
    - sed -i "s|gitlab.*|${DOCKER_IMG}|" node-rolling-update.yml
    - kubectl apply -f node-rolling-update.yml
  when: manual
  only: 
    - devlopment-integration


stages:
  - build-image
  - deploy


docker_build_development:
  stage: build-image
  tags:
    - kube-ssh
  script:
    - cp /home/config/dev/fruit-chop-node/.env .
    - cp /home/config/dev/fruit-chop-node/Dockerfile .
    - cp /home/config/dev/fruit-chop-node/node-rolling-update.yml .
    - cp -r /home/config/dev/fruit-chop-node/cert .
    - docker build -f Dockerfile -t $CI_REGISTRY_IMAGE:$CI_COMMIT_SHORT_SHA .
    - echo $CI_REGISTRY_IMAGE
    - docker login -u $CI_REGISTRY_USER -p $CI_REGISTRY_PASSWORD $CI_REGISTRY
    - docker push $CI_REGISTRY_IMAGE:$CI_COMMIT_SHORT_SHA
    - DOCKER_IMG="$CI_REGISTRY_IMAGE:$CI_COMMIT_SHORT_SHA"
    - echo $DOCKER_IMG
    - sed -i "s|gitlab.*|${DOCKER_IMG}|" node-rolling-update.yml
    - kubectl apply -f node-rolling-update.yml
  when: manual
  only:
    - development



